{
  "name": "browserify-babel-setup",
  "version": "0.0.0",
  "description": "An example project setup for using ES6 features in any application using browserify and babel transpiler",
  "main": "index.js",
  "scripts": {
    "start": "watchify -o bundle.js -v -d scripts/demo.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexnm/browserify-babel-setup.git"
  },
  "author": "",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/alexnm/browserify-babel-setup/issues"
  },
  "dependencies": {
    "browserify": "~10.2.6",
    "babelify": "~6.1.3",
    "watchify": "~3.2.3"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
